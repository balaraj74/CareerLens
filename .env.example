# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google AI (Gemini) API Key for Genkit
GOOGLE_GENAI_API_KEY=your_gemini_api_key
GEMINI_API_KEY=your_gemini_api_key

# Google Maps API Key (for library finder feature)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google OAuth Client ID
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_oauth_client_id

# Next.js Server Actions Encryption Key (optional, auto-generated if not provided)
# NEXT_SERVER_ACTIONS_ENCRYPTION_KEY=

# Google Custom Search API
NEXT_PUBLIC_GOOGLE_SEARCH_API_KEY=your_google_search_api_key
NEXT_PUBLIC_GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id

# NewsAPI.org API Key (for news feature)
# Get your free API key at: https://newsapi.org/register
NEWS_API_KEY=your_newsapi_key

# Environment
NODE_ENV=development
